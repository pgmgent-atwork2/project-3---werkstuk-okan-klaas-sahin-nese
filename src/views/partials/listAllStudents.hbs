{{#each allUserMetas}}
{{#if adres}}
<ul>
    <li>{{id}}</li>
   
    <li>{{voornaam}} {{achternaam}}</li>
    {{#if student}}
    <li class="avatar__list"> <img src="{{student.avatar}}" alt="avatar"></li>
    <li>  {{student.email}}</li>
    {{/if}}
    
    <li>{{geboortedatum}} </li>
    <li> {{geboorteplaats}} </li>
     
    <li>{{classroom}}</li>
    <li> {{adres}} </li>
    <li>
        <a href="/editstudent/{{student.id}}"><img src="assets/img/edit.svg" alt="change"></a>
        <img src="assets/img/delete.svg" alt="delete">
    </li>
</ul>
{{/if}}
{{/each}}