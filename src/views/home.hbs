<body >

    <div class="background__right background__left ">

    </div>
{{> header}}
<main>
    <div class="home__space-area">
    {{> headCategory}}
    </div>
</main>
<pre>{{users}}</pre>

<h1>Welkom, {{user.email}}!</h1>

{{#if (eq userRole "admin")}}
  <!-- Inhoud specifiek voor beheerders -->
  <p>Je bent een beheerder.</p>
{{else if (eq userRole "teacher")}}
  <!-- Inhoud specifiek voor leraren -->
  <p>Je bent een leraar.</p>
{{else}}
  <!-- Standaardinhoud voor andere rollen -->
  <p>Je hebt een student.</p>
{{/if}}


</body>

