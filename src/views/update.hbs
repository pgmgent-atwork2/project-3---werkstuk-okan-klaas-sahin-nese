<body>
  {{> header}}
<div class="update_box">
  <h2>Update</h2>
  {{>form-errors this}}
  <form method="post" name:"role" >
    {{#each inputs}}
      {{>form-input this}}
    {{/each}}
    <div class="footer-container">
      <input type="reset" class="btn btn-tertiary" value="Clear">
      <input type="submit" class="btn btn-primary" value="Register">
    </div>
  </form>
</div>  
</body>